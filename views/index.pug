extends layout

block content
  .container 
  h1 VATSIM Data Harvester         
    table.table
      thead.thead-light
        tr
          th(scope='col') Callsign
          th(scope='col') Name
          th(scope='col') Role
      tbody
        each val in data
          tr
            td #{val.callsign}
            td #{val.realname}
            td #{val.clienttype}